  updated_at              DateTime        @default(now()) @db.Timestamp(0)
  last_check_time         DateTime        @default(dbgenerated("('0000-00-00 00:00:00')")) @db.Timestamp(0)
  access_token_expires_at DateTime        @default(dbgenerated("('0000-00-00 00:00:00')")) @db.Timestamp(0)
